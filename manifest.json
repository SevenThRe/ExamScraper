{
    "manifest_version": 3,
    "name": "ExamScraper",
    "version": "1.0.0",
    "description": "考试题目导出工具",
    "permissions": [
      "activeTab",
      "downloads",
      "scripting",
      "declarativeContent",
      "storage",
      "*://*.omniedu.com/*"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "scripts/background.js",
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["*://*.omniedu.com/*"],
        "js": ["scripts/content.js"],
        "run_at": "document_end"
      }
    ],
    "web_accessible_resources": [{
      "resources": ["icons/*"],
      "matches": ["<all_urls>"]
    }]
  }